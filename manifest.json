{
    "manifest_version": 2,
    "name": "VidSpeed",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["vidspeed/content.js"]
        }
    ],
    "background": {
        "scripts": ["vidspeed/background.js"]
    },
    "browser_action": {
        "default_popup": "vidspeed/popup.html"
    },
    "description": "Speed up videos"
  }